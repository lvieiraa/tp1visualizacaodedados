#include <stdio.h>

typedef struct Lugar{
    char nomeLugar;
    int numVisitas;
    }lugar;
    
    typedef struct Pessoa{
    lugar local[99];
}pessoa;
 
 int existeLocal(pessoa *pe, char lugar, int ind){
     int i;
     for(i=0;i<ind;i++)
        if(lugar == pe.local[i].nomeLugar){
            return 1;
            
        }
     return 0;
     }

int main()
{int i, j, k=10;
i=1; j=1;
   pessoa pe[k];
   
   for(i=1;i<k;i++)
        while(fscanf (alguma coisa) !=i){
        
        int aux = existeLocal(pe[i], char lido, j);
        if(aux != 0)
        pe[i].local[aux].numVisitas ++;
        else 
        pe[i].local[j].nomeLugar = char lido ;
